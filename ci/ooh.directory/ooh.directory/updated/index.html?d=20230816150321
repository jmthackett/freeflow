<script>
      /**
       * Keep the times ago that each item was updated, udpated.
       * Tweaked from https://gist.github.com/adactio/ccca5710152e77fb26b4a4df0ea8fc1e
       * Also see https://adactio.com/journal/20377
       */
      (function (win, doc) {
        'use strict';
        if (!doc.querySelectorAll || !win.Intl || !win.Intl.RelativeTimeFormat) {
          // doesn't cut the mustard.
          return;
        }

        // Which elements do we want to target?
        // e.g. "time[datetime]"
        var selector = ".js-timeago";

        var rtf = new Intl.RelativeTimeFormat('en', {
          localeMatcher: 'best fit',
          numeric: 'always',
          style: 'long'
        });
        var units = {
          'year'      : 1000 * 24 * 60 * 60 * 365,
          'month'     : 1000 * 24 * 60 * 60 * (365/12),
          'week'      : 1000 * 24 * 60 * 60 * 7,
          'day'       : 1000 * 24 * 60 * 60,
          'hour'      : 1000 * 60 * 60,
          'minute'    : 1000 * 60,
          // So that anything under 1 minute is shown as "just now":
          //'second'    : 1000
        };
        var unit, then, now, diff, amount, text;
        var updateDatetimes = function () {
          now = Date.now();
          doc.querySelectorAll(selector).forEach(function(timeElement) {
            text = 'just now';
            then = timeElement.getAttribute('datetime');
            diff = new Date(then) - new Date(now);
            for (unit in units) {
              amount = units[unit];
              if (Math.abs(diff) > amount) {
                text = rtf.format(Math.round(diff / amount), unit);
                break;
              }
            }
            timeElement.innerText = text;
          });
        };
        updateDatetimes();
        // Run the update every 30 seconds
        setInterval(updateDatetimes, 1000 * 30);
      }(this, this.document));
    </script>
</body>
</html>
